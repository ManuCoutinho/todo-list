{
  "name": "todo-list",
  "version": "1.0.0",
  "engines": {
    "node": "16.15.1",
    "npm": "8.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuCoutinho/todo-list"
  },
  "homepage": "https://ourlist.netlify.app",
  "author": "Emanuela Coutinho",
  "description": "to do list app",
  "keywords": [
    "react",
    "vite",
    "styled components",
    "to do",
    "node",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint  src",
    "prepare": "husky install",
    "test": "jest --watch",
    "coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "eslint src --fix"
    ],
    "*.{json, md, yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "polished": "^4.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4",
    "vite": "^3.1.8"
  },
  "main": "index.html",
  "license": "MIT"
}
